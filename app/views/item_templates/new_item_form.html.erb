<h1> Add a new item </h1>

<form action = "/insert_item" method="post">

  <div>
    <label for="link_url">Link URL</label>
    <input id="link_url" type="text" name="link_url" placeholder = "Enter a URL...">
  </div>

<div>
  <label for="link_description">Link Description</label>
  <input id="link_description" type = "text" name="link_description" placeholder = "Enter a link description...">
</div>

<div>
  <label for="thumbnail">Thumbnail URL</label>
  <input id="thumbnail" type="text" name="thumbnail_url" placeholder = "Enter a thumbnail URL...">
</div>

<button>Create item</button>
</form>
